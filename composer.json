{
    "name": "dmp/audit-request-bundle",
    "description": "Bundle for support of Audit Request",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "dmp/aop-bundle": "^0.1",
        "dmp/cg-lib": "^0.1",
        "dmp/cqrs-sf-bundle": "^0.5",
        "dmp/paginator-bundle": "^0.1.0",
        "dmp/transactional-bundle": "^0.2.1",
        "doctrine/orm": "^2.13.2",
        "doctrine/annotations": "^1.13",
        "symfony/http-kernel": "^5.4 || ^6.2",
        "symfony/uid": "^5.4 || ^6.2",
        "symfony/validator": "^5.4 || ^6.2",
        "symfony/orm-pack": "^2.4",
        "symfony/security-bundle": "^5.4 || ^6.2"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "matthiasnoback/symfony-dependency-injection-test": "^4.3",
        "phpunit/phpunit": "^9.5 || ^10.1"
    },
    "autoload": {
        "psr-4": {
            "DMP\\AuditRequestBundle\\":  "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DMP\\AuditRequestBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-cc": "phpunit coverage-html report/"
    }
}
